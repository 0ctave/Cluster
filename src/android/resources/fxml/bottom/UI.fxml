<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
      <BorderPane fx:id="controls" id="controls" maxHeight="48.0" prefHeight="48" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml" fx:controller="fr.octave.fr.octave.cluster.controller.UI">

         <stylesheets>
            <URL value="@/styles/control.css" />
         </stylesheets>
         <center>
            <BorderPane maxWidth="300.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <center>
                  <Button id="newUI" onAction="#loadNew" maxHeight="48" maxWidth="48" minHeight="48" minWidth="48" mnemonicParsing="false" BorderPane.alignment="CENTER" />
               </center>
               <left>
                  <Button id="homeUI" onAction="#loadHome" maxHeight="48" maxWidth="48" minHeight="48" minWidth="48" mnemonicParsing="false" BorderPane.alignment="CENTER" />
               </left>
               <right>
                  <Button id="discoverUI" onAction="#loadDiscover" maxHeight="48" maxWidth="48" minHeight="48" minWidth="48" mnemonicParsing="false" BorderPane.alignment="CENTER" />
               </right>
            </BorderPane>
         </center>
      </BorderPane>
